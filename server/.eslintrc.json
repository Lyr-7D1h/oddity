{
  "env": {
    "node": true,
    // "browser": true,
    "commonjs": true,
    "es6": true
  },
  "root": true,
  "plugins": ["@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "impliedStrict": true,
    "ecmaVersion": 2018
  },
  "rules": {}
}

{
  "name": "Forum",
  "version": "0.0.1",
  "routes": [
    {
      "path": "/",
      "component": "/client/components/pages/RootPage"
    },
    {
      "path": "/drafts",
      "component": "/client/components/pages/DraftsPage"
    },
    {
      "path": "/:category/",
      "component": "/client/components/pages/CategoryPage"
    },
    {
      "path": "/:category/:thread/",
      "component": "/client/components/pages/ThreadPage"
    },
    {
      "path": "/:category/:thread/:post/",
      "component": "/client/components/pages/PostPage"
    }
  ],
  "adminPage": "/client/components/pages/AdminPage"
}
